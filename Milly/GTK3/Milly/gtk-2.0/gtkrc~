style "notebook" {

  xthickness		= 0
  ythickness		= 0

  fg[NORMAL]		= @fg_color
  fg[ACTIVE]		= @selected_fg_color
  bg[ACTIVE]		= @notebook_selected_bg_color

engine "pixmap" 
  {
    image 
    {
        function		= EXTENSION
		state			= ACTIVE
		file			= "notebook_inactive.png"
		border			= { 0,0,0,0 }
		stretch			= TRUE
		gap_side		= TOP
    }
    image 
    {
        function		= EXTENSION
		state			= ACTIVE
		file			= "notebook_inactive.png"
		border			= { 0,0,0,0 }
		stretch			= TRUE
		gap_side		= BOTTOM
    }
    image 
    {
        function		= EXTENSION
		state			= ACTIVE
		file			= "notebook_inactive.png"
		border			= { 0,0,0,0 }
		stretch			= TRUE
		gap_side		= RIGHT
    }
    image 
    {
        function		= EXTENSION
		state			= ACTIVE
		file			= "notebook_inactive.png"
		border			= { 0,0,0,0 }
		stretch			= TRUE
		gap_side		= LEFT
    }	
    image 
    {
        function		= EXTENSION
		file			= "notebook_active.png"
		border			= { 0,0,0,0 }
		stretch			= TRUE
		gap_side		= BOTTOM
    }
    image 
    {
        function		= EXTENSION
		file			= "notebook_active.png"
		border			= { 0,0,0,0 }
		stretch			= TRUE
		gap_side		= TOP
    }
    image 
    {
        function		= EXTENSION
		file			= "notebook_active.png"
		border			= { 0,0,0,0 }
		stretch			= TRUE
		gap_side		= RIGHT
    }
    image
    {
        function		= EXTENSION
		file			= "notebook_active.png"
		border			= { 0,0,0,0 }
		stretch			= TRUE
		gap_side		= LEFT
    }
    image 
    {
        function			= BOX_GAP
		file				= "notebook_inactive.png"
		border				= { 0,0,0,0 }
		stretch				= TRUE
		gap_file			= "notebook_inactive.png"
		gap_border     			= { 0,0,0,0 }
		gap_side			= TOP
    }
    image 
    {
        function			= BOX_GAP
		file				= "notebook_inactive.png"
		border				= { 0,0,0,0 }
		stretch				= TRUE
		gap_file			= "notebook_inactive.png"
		gap_border			= { 0,0,0,0 }
		gap_side			= BOTTOM
    }
    image 
    {
        function			= BOX_GAP
		file				= "notebook_inactive.png"
		border				= { 0,0,0,0 }
		stretch				= TRUE
		gap_file			= "notebook_inactive.png"
		gap_border			= { 0,0,0,0 }
		gap_side			= LEFT
    }
    image 
    {
        function			= BOX_GAP
		file				= "notebook_inactive.png"
		border				= { 0,0,0,0 }
		stretch				= TRUE
		gap_file			= "notebook_inactive.png"
		gap_border			= { 0,0,0,0 }
		gap_side			= RIGHT
    }

# How to draw the box of a notebook when it isnt attached to a tab

    image 
    {
		function			= BOX
		file				= "notebook_inactive.png"
		border				= { 0,0,0,0 }
		stretch				= TRUE
    }

    image 
    {
        function			= FOCUS
		file				= "notebook_inactive.png"
		border				= { 0,0,0,0 }
		stretch				= TRUE
    }

}		
}


