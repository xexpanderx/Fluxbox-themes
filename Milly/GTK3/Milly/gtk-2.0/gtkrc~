## Default colors
gtk-color-scheme = "base_color:#0f1013\nbg_color:#0f1013\nscrollbar_color:#D53679"
gtk-color-scheme = "text_color:#D2D2D2\nfg_color:#D2D2D2"
gtk-color-scheme = "tooltip_bg_color:#0f1013\ntooltip_fg_color:#0f1013\nlink_color:#20474C\nvisited_link_color:#D53679"
## Toolbar colors
gtk-color-scheme = "toolbar_bg_color:#0f1013"

## Treeview colors
gtk-color-scheme = "treeview_bg_color:#0f1013"
gtk-color-scheme = "treeview_header_selected_bg_color:#D53679"
gtk-color-scheme = "treeview_header_text_selected_bg_color:#FFFFFF"
## Handle colors
gtk-color-scheme = "handle_bg_color:#D53679"

## Dark-Flat
gtk-color-scheme = "theme_bg_dark_color:#0f1013\ntheme_fg_dark_color:#0f1013\ntheme_text_dark_color:#0f1013"

##Text colors
gtk-color-scheme = "other_text_color:#D2D2D2"
gtk-color-scheme = "other_text_color2:#0f1013"

## Menu color selection
gtk-color-scheme = "menu_selected_bg_color:#D53679"
gtk-color-scheme = "menubar_selected_bg_color:#D53679"
gtk-color-scheme = "menubar_bg_color:#0f1013"
gtk-color-scheme = "menubar_fg_color:#D2D2D2"
gtk-color-scheme = "menubar_fg_selected_color:#FFFFFF"

## selection color
gtk-color-scheme = "selected_fg_color:#FFFFFF"
gtk-color-scheme = "selected_bg_color:#D53679"

## Notebook colors
gtk-color-scheme = "notebook_selected_bg_color:#D53679"

## Spinbutton colors
gtk-color-scheme = "spinbutton_fg_color:#D2D2D2"
gtk-color-scheme = "spinbutton_selected_bg_color:#D53679"
gtk-color-scheme = "spinbutton_bg_color: #2C2C2C"

## Scale colors
gtk-color-scheme = "scale_bg_color:#2C2C2C"
gtk-color-scheme = "scale_bg_prelight_color:#D53679"
gtk-color-scheme = "scale_bg_selected_color:#D53679"
gtk-color-scheme = "scale_slider_bg_color:#2C2C2C"

## Firefox and select text marker
gtk-color-scheme = "firefox_selected_color:#D53679"

## Button
gtk-color-scheme = "button_prelight_fg_color:#D2D2D2"
gtk-color-scheme = "button_prelight_bg_color:#D53679"
gtk-color-scheme = "button_label_prelight_bg_color:#D53679"

## Entry
gtk-color-scheme = "entry_bg_color:#2c2c2c"

## Progressbar
gtk-color-scheme = "progressbar_bg_selected_color:#D53679"

## checkradio
gtk-color-scheme = "checkradio_bg_color: #2C2C2C"
gtk-color-scheme = "checkradio_selected_bg_color: #d53679"
###################################################################

gtk-auto-mnemonics	= 1

style "default" {
	xthickness	= 0
	ythickness	= 0

	#######################
	# Style Properties
	#######################
	GtkButton   			::child-displacement-x			= 0
	GtkButton				::child-displacement-y			= 0
	GtkButton				::default-border				= {1, 1, 1, 1}
	GtkButton				::image-spacing					= 4

	GtkCheckButton			::indicator-size				= 16
	GtkRadioButton			::indicator-size				= 16

	GtkEntry				::honors-transparent-bg-hint	= 1
	#GtkEntry				::invisible-char				= 0x2022
	GtkEntry				::progress-border				= {2, 2, 2, 2}
    GtkEntry    			::state-hint        			= 0

	GtkProgressBar			::min-vertical-bar-width		= 13
	GtkProgressBar			::min-horizontal-bar-height		= 13

	GtkExpander				::expander-size					= 14

	GtkMenuBar  			::internal-padding				= 0
	GtkMenuBar  			::window-dragging				= 1
	GtkMenuBar				::shadow-type					= GTK_SHADOW_NONE
	GtkMenu					::horizontal-padding			= 0
	GtkMenu					::vertical-padding				= 0

	GtkNotebook				::tab-overlap					= 0		# Default = 2
	GtkNotebook				::tab-curvature					= 0		# Default = 1
	GtkNotebook				::arrow-spacing					= 0

	GtkRange				::trough-border					= 0
	GtkRange				::slider-width					= 15
	GtkRange				::stepper-size					= 1

	GtkScale				::slider-length					= 14
	GtkScale				::slider-width					= 14
	GtkScale				::trough-side-details			= 1

	GtkScrollbar    		::has-backward-stepper			= 0  
	GtkScrollbar			::has-forward-stepper			= 0   # El maldito rollo de gtk2 y las scrollbars
	GtkScrollbar			::min-slider-length				= 10
	GtkScrollbar			::slider-width					= 5
	GtkScrollbar			::trough-border					= 5
	GtkScrollbar			::activate-slider 				= FALSE
	GtkScrollbar			::trough-side-details			= 0
	GtkScrollbar			::stepper-size              	= 0

	GtkScrolledWindow		::scrollbar-spacing				= 0
	GtkScrolledWindow		::scrollbars-within-bevel		= 0

	GtkToolbar				::internal-padding				= 0
	GtkToolbar				::window-dragging				= 1
	
	GtkWidget				::link-color 					= @link_color
	GtkWidget				::visited-link-color 			= @visited_link_color
    GtkWidget				::focus-padding					= 0
#	GtkWidget				::interior-focus				= FALSE
#	GtkWidget				::wide-separators				= TRUE
	GtkWidget				::separator-height				= 0
	GtkWidget				::separator-width				= 0
	WnckTasklist			::fade-overlay-rect				= 0
    
    GtkWindow               ::resize-grip-height        	= 0 # Ocultar resize-grip
    GtkWindow           	::resize-grip-width         	= 0
    	
    XfceNotifyWindow		::border-color 					= @bg_color
    XfceNotifyWindow		::border-radius 				= 0
    XfceNotifyWindow		::border-width 					= 0.0
    XfceNotifyWindow		::border-color-hover 			= @bg_color
    XfceNotifyWindow		::border-width-hover 			= 0.0

	GtkLabel				::text-shadow 					= none

	####################
	# Color Definitions
	####################

	bg[NORMAL]          = @bg_color
	bg[PRELIGHT]		= @button_label_prelight_bg_color
	bg[SELECTED]		= @menu_selected_bg_color
	bg[INSENSITIVE] 	= @bg_color
	bg[ACTIVE]         	= @bg_color

	fg[NORMAL]          = @fg_color
	fg[PRELIGHT]		= @selected_fg_color
	fg[SELECTED]		= @selected_fg_color

	fg[INSENSITIVE] 	= shade ( 0.5 , @other_text_color)
	fg[ACTIVE]			= @selected_fg_color

	text[NORMAL]		= @fg_color
	text[PRELIGHT]		= @selected_fg_color
	text[SELECTED]		= "#FFFFFF"
	text[INSENSITIVE]	= @fg_color
	text[ACTIVE]		= @selected_fg_color

	base[NORMAL]		= @base_color
	base[PRELIGHT]		= @base_color
	base[SELECTED]		= @firefox_selected_color
	base[INSENSITIVE]	= @bg_color
	base[ACTIVE]		= @selected_bg_color
	
	engine "murrine" {
		animation				= FALSE
		arrowstyle				= 0			# Estilo flechas menu (triangulos)
		border_shades      		= {1.0, 1.0}
		contrast				= 0.0
		comboboxstyle 			= 0			# 0 = normal combobox, 1 = colorized combobox button		
		expanderstyle			= 1			# Treview expander (triangulo)
		focusstyle				= 0 # 0 to disable focus drawing. 
								# 1 to use dots. 
								# 2 to use a small colored rectangle. 
								# 3 to use a colored rectangle that touches the borders.
		focus_color 			= @base_color
		glazestyle				= 0 # 0 = flat highlight, 
								# 1 = curved highlight
								# 2 = concave style, 
								# 3 = top curved highlight, 
								# 4 = beryl highlight
		glowstyle				= 4 # 0 = top, 1 = bottom, 2 = top and bottom, 3 = horizontal, 4 = centered glow
		glow_shade				= 1.0
  		gradient_shades			= {1.00, 1.00, 1.00, 1.00}              
		highlight_shade			= 1.0
		lightborder_shade		= 1.0
		lightborderstyle		= 0 # 0 = top side, 1 = all sides
		listviewstyle			= 0 # 0 = nothing, 1 = dotted, 2 = line
		menubaritemstyle		= 0 # 0 = menuitem look, 1 = button look
		menubarstyle			= 0 # 0 = flat, 1 = glassy, 2 = gradient, 3 = striped
		menuitemstyle			= 0 # 0 = flat, 1 = glassy, 2 = striped
		menustyle				= 0 # 0 = no vertical menu stripe, 1 = display vertical menu stripe, 2 = faint? 3 = borderless
		progressbarstyle		= 0
		reliefstyle				= 0  # 0 = flat, 1 = inset, 2 = shadow, 3 = shadow with gradient, 4 = stronger shadow with gradient
		rgba					= FALSE	# alpha transparency
		roundness				= 0 # 0 = square
		scrollbarstyle			= 0 # 0 = nothing
		separatorstyle 			= 1	# 0 = Hard seperators 1 = Smooth seperators
		shadow_shades			= {1.0, 1.0}
		sliderstyle		   		= 0 # 0 = nothing added
		stepperstyle			= 0 # 0 = standard, 1 = integrated stepper handles, 2 = unknown
		toolbarstyle			= 0 # 0 = flat, 1 = glassy, 2 = gradient
		trough_shades			= {1.0, 1.0}
		text_shade				= 1.0
		cellstyle 				= 0	
		handlestyle				= 0
		border_colors 			= FALSE
		gradient_colors			= FALSE
			
	} 	

	
}

