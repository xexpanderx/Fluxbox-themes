style "font_button" {
	xthickness			= 0
	ythickness			= 0

    fg[NORMAL]         	= @other_text_color
    fg[PRELIGHT]		= @selected_fg_color
	fg[INSENSITIVE] 	= shade (0.70, @other_text_color)

	bg[PRELIGHT]		= @menubar_selected_bg_color
	bg[SELECTED]		= @menubar_selected_bg_color
	bg[INSENSITIVE] 	= @menubar_selected_bg_color
	bg[ACTIVE]         	= @menubar_selected_bg_color

	base[PRELIGHT]		= @menubar_selected_bg_color
	base[SELECTED]		= @menubar_selected_bg_color
	base[INSENSITIVE] 	= @menubar_selected_bg_color
	base[ACTIVE]        = @menubar_selected_bg_color

	engine "murrine" {
		separatorstyle 			= 0	# 0 = Hard seperators 1 = Smooth seperators	
	}
}
