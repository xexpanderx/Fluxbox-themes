	GtkTreeView			::even_row_color			= @base_color
	GtkTreeView			::even_col_color			= @base_color
	GtkTreeView			::odd_col_color			    = @base_color
	GtkTreeView			::grid-line-width			= 0
	GtkTreeView			::tree-line-width			= 0
	GtkTreeView			::horizontal-separator		= 2
	GtkTreeView			::vertical-separator		= 2

	GtkTreeView			::expander_size				= 0
	GtkTreeView			::row_ending_details 		= 1

	base[SELECTED]  	= @selected_bg_color		
	base[ACTIVE]  		= @selected_bg_color		

	fg[NORMAL]			= @other_text_color			
	fg[PRELIGHT]		= @treeview_header_text_selected_bg_color

	bg[NORMAL]   		= @treeview_bg_color
	bg[SELECTED]		= @progressbar_bg_selected_color		
	text[NORMAL]    	= @fg_color				
	text[SELECTED]  	= @selected_fg_color					
	text[ACTIVE] 		= @selected_fg_color

	engine "pixmap"
	{
		# Menuitem background (mouse over)
		image
		{	
			function		= FLAT_BOX
			recolorable		= TRUE
			file			= "treeviewer_select.png"
			border			= { 0, 0, 0, 0}
			stretch			= TRUE
			shadow		    = NONE
			state			= SELECTED
		}
		image
		{
			function		= FOCUS
			recolorable		= TRUE
			file			= "treeviewer_focus.png"
			border			= { 0, 0, 0, 0}
			stretch			= TRUE
		}

       }				
}

